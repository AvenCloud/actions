{
  "editor.formatOnSave": true,

  "files.insertFinalNewline": false,

  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "eslint.packageManager": "yarn",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "Aven",
    "Retryable",
    "USERPROFILE",
    "artifactcache",
    "promisify",
    "sourcemap",
    "windir"
  ]
}
