{
  "name": "aven-github-actions",
  "version": "0.0.0",
  "author": "Cameron Tacklind <cameron@tacklind.com>",
  "license": "Apache 2.0",
  "private": true,
  "description": "GitHub Actions that do more in fewer steps",
  "keywords": [
    "actions",
    "node",
    "git",
    "clone",
    "commit",
    "cache"
  ],
  "scripts": {
    "build": "ts-node --files --dir scripts build",
    "lint": "eslint \"**/*\"",
    "format": "prettier \"**/*\"",
    "spell": "cspell \"**/*\"",
    "format-check": "npm run format -- --check",
    "clean": "rm -rf dist"
  },
  "engines": {
    "yarn": "no. Use Npm here."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AvenCloud/actions.git"
  },
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "@actions/http-client": "^1.0.8",
    "@actions/io": "^1.0.2",
    "github-username-regex": "^1.0.0",
    "ts-node": "^8.10.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/github-username-regex": "^1.0.0",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "^14.0.13",
    "@types/rmfr": "^2.0.0",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@zeit/ncc": "^0.22.3",
    "cspell": "^4.0.63",
    "eslint": "^7.2.0",
    "eslint-plugin-github": "^4.0.1",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "yarn-or-npm": "^3.0.1"
  }
}
